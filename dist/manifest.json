{
    "manifest_version":3,
    "name":"Better Babechat",
    "version": "0.0.0",
    "permissions": [
        "clipboardWrite"
    ],
    "content_scripts": [{
        "js": ["./index.js"],
        "matches": ["https://www.babechat.ai/*","https://babechat.ai/*"],
        "type": "module"
    }],
    "action": {
        "default_popup": "popup.html"
    }
}